<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.TextFlow?>

<FlowPane columnHalignment="CENTER" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apc.com.supervisioncompraspublicas.EtiquetadoController">
   <children>
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <Button fx:id="evaluar" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#evaluar" text="Etiquetar" />
            <TextField fx:id="textoEvaluar" prefHeight="26.0" prefWidth="402.0" />
         </children>
         <FlowPane.margin>
            <Insets />
         </FlowPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <TextFlow fx:id="display" prefHeight="195.0" prefWidth="498.0" textAlignment="JUSTIFY" />
   </children>
</FlowPane>
